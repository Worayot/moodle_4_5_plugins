<div class="pd-16">
  <h2>Course Insights</h2>

  <div class="analytics-section">
    <h3>Course Bookmark Overview:</h3>
    <div class="table-responsive">
      <table class="table analytics-table table-striped">
        <thead>
          <tr>
            <th>Course Name</th>
            <th>Saved (Not Started)</th>
          </tr>
        </thead>
        <tbody>
  {{#courses}}
  <tr>
    <td>{{name}}</td>
    <td>{{saved_count}}</td>
  </tr>
  {{/courses}}
  {{^courses}}
  <tr>
    <td colspan="2">No data available</td>
  </tr>
  {{/courses}}
</tbody>
      </table>
    </div>
  </div>
</div>
